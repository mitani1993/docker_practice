{
  "name": "remote-java",
  "dockerComposeFile": "../compose.yml",
  "service": "java",
  "workspaceFolder": "/srv",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      },
      "extensions": [
        "vscjava.vscode-java-pack",
        "pivotal.vscode-boot-dev-pack",
        "vscjava.vscode-lombok"
      ]
    }
  }
}
